    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="firebaseMessageValidate" xsi:type="object">MageStack\FirePush\Console\Command\Validate</item>
            </argument>
        </arguments>
    </type>
</config>